  "devDependencies": {
    "@types/ace": "^0.0.52",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.5.1",
    "@types/sortablejs": "^1.15.8",
    "concurrently": "^8.2.2",
    "ejs": "^3.1.10",
    "ejs-lint": "^2.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "less": "^4.2.0",
    "less-watch-compiler": "^1.16.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  },
